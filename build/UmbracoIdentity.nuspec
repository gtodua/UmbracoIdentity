<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>UmbracoIdentity</id>
    <title>ASP.Net Identity for Umbraco Members</title>
    <version>0.0.0.0</version>
    <authors>Shannon Deminick</authors>
    <description>A library for enabling ASP.Net Identity to work with Umbraco front-end members</description>
    <summary>A library for enabling ASP.Net Identity to work with Umbraco front-end members</summary>
    <language>en-US</language>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://github.com/Shandem/UmbracoIdentity/</projectUrl>
    <tags>asp.net identity umbraco security authentication authorization owin</tags>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>

      <dependency id="UmbracoCms.Core" version="7.3.4" />    
      
    </dependencies>
  </metadata>
  <files>

    <file src="*.dll" target="lib\net45" />
    <file src="*.pdb" target="lib\net45" />

    <file src="nuget-transforms\web.config.install.xdt" target="Content" />

    <file src="Controllers\*.pp" target="content\Controllers" />
    <file src="Models\**\*.pp" target="content\Models" />
    <file src="Views\**\*.pp" target="content\Views" />
    <file src="Scripts\**\*.js" target="content\Scripts" />
    <file src="App_Start\**\*.pp" target="content\App_Start" />

    <file src="Readme.txt" target="" />

  </files>
</package>